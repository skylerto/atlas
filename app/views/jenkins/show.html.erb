<div class="page-header">
  <%= link_to jenkin_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_jenkin_path(@jenkin), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show jenkin</h1>
</div>

<dl class="dl-horizontal">
  <dt>Server ip:</dt>
  <dd><%= @jenkin.server_ip %></dd>

  <dt>Username:</dt>
  <dd><%= @jenkin.username %></dd>

  <dt>Password:</dt>
  <dd><%= @jenkin.password %></dd>

</dl>
