<div class="page-header">
  <%= link_to jobs_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_job_path(@job), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show job</h1>
</div>

<dl class="dl-horizontal">
  <dt>Server ip:</dt>
  <dd><%= @job.server_ip %></dd>

  <dt>Username:</dt>
  <dd><%= @job.username %></dd>

  <dt>Password:</dt>
  <dd><%= @job.password %></dd>

</dl>
