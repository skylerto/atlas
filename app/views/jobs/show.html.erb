<div class="container build_history">
  <%# Build History %>
  <% @job.builds.each do |build| %>
    <div class="job row">
      <div class="col-md-2">
        <%= build.author_full_name %>
      </div>
      <div class="col-md-10">
        <div class="container">
          <div class="row">
            <div class="col-md-1 fill-<%= build.status.downcase %>">
              <%= raw build.icon %>
            </div>
            <div class="col-md-11 display_name">
              <%= link_to build.display_name, build_link(build) %>
            </div>
          </div>
        </div>
      </div>
    </div>
  <% end %>



  <div class="col-md-10">
    <div class="page-header">
      <h1><%= @job.current_build.full_display_name %></h1>
    </div>
    <div class="code_container">
      <pre>
        <%= @job.current_build.console_output %>
      </pre>
    </div>
  </div>
</div>
