<div class="container build_history">
  <%# Build History %>
  <% @job.builds.each do |build| %>
    <div class="job row">
      <div class="container">
        <div class="row">
          <div class="col-md-1 fill-<%= build.status.downcase %>">
            <%= raw build.icon %>
          </div>
          <div class="col-md-11">
            <div class="display_name">
              <%= build.comment %>
            </div>
            <div class="metadata">
              <span class="version">
                <%= link_to build.display_name, build_link(build) %>
              </span>
              <span class="time">
                <% if build.date.instance_of? String %>
                  <%= build.date %>
                <% else %>
                  <%= time_ago_in_words build.date %> ago
                <% end %>
              </span>
              by
              <span class="author">
                <%= build.author_full_name %>
              </span>
            </div>
          </div>
        </div>
      </div>
    </div>
  <% end %>



  <div class="col-md-10">
    <div class="page-header">
      <h1><%= @job.current_build.full_display_name %></h1>
    </div>
    <div class="code_container">
      <pre>
        <%= @job.current_build.console_output %>
      </pre>
    </div>
  </div>
</div>
